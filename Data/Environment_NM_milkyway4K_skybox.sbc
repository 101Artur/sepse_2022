        <ShadowCascadeMaxDistanceMultiplierMedium>3</ShadowCascadeMaxDistanceMultiplierMedium>
        <ShadowCascadeMaxDistanceMultiplierHigh>5</ShadowCascadeMaxDistanceMultiplierHigh>
