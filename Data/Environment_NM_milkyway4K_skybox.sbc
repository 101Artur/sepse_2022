    <PlanetProperties>
        <AtmosphereIntensityMultiplier>16</AtmosphereIntensityMultiplier>
        <AtmosphereIntensityAmbientMultiplier>9</AtmosphereIntensityAmbientMultiplier>
        <CloudsIntensityMultiplier>15</CloudsIntensityMultiplier>
    </PlanetProperties>
